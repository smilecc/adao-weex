source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'
platform :ios, '8.0'
#忽略引入库的警告
inhibit_all_warnings!

def common
    #WeexSDK
    pod 'WeexSDK', :git => 'https://github.com/bmfe/WeexiOSSDK.git', :tag => '0.18'
    #Weex debugger 调试工具，只在开发模式集成
    pod 'WXDevtool', '0.15.3', :configurations => ['Debug']
    #Eros iOS 基础库
    pod 'BMBaseLibrary', :git => 'https://github.com/bmfe/Benmu-iOS-Library.git', :tag => '1.2.4'

    pod 'WeexActionSheet'
    pod 'ZLPhotoBrowser', '2.6.4'
    pod 'WeexImagePicker'
    #Other Plugins

   end

target 'WeexEros' do
    common
end
